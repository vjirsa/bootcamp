\long\def\bform#1\eform{\begin{equation}

   \begin{minipage}{\eqbreite}\vskip .1cm

   \let\\ = \thcr

   \halign{$\displaystyle{##}$ \hfil

  && $\displaystyle{##}$\hfil\cr#1\cr}

   \vskip .3cm \end{minipage}

\end{equation}}

\def\thcr{\cr\noalign{\vskip.3cm}}

